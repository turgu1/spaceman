%tr
  %td= allocation_row.space.name
  %td= allocation_row.consumer_org_name
  %td.nowrap= allocation_row.consumer_name
  %td.nowrap.hcenter= allocation_row.from_date
  %td.nowrap.hcenter= allocation_row.to_date || ''
  %td= allocation_row.notes
  %td.nowrap.no-print= all_actions allocation_row, t('.of_space_to', item_from: allocation_row.space.name, item_to:allocation_row.consumer_name), { 'data-right-column-state': true, 'data-center-column-state': true }
